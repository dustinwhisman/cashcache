  "license": "ISC",
  "dependencies": {
    "firebase-admin": "^11.6.0",
    "mongodb": "^5.2.0",
    "stripe": "^12.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.3.0"
  }
}
