  "license": "ISC",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "mongodb": "^6.5.0",
    "stripe": "^14.22.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.1.2",
    "dotenv": "^16.4.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.3"
  }
